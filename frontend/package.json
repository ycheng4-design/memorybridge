{
  "name": "memorybridge-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:spatial": "XR_ENV=avp vite",
    "build": "tsc --project tsconfig.app.json && vite build",
    "build:spatial": "XR_ENV=avp tsc --project tsconfig.app.json && XR_ENV=avp vite build",
    "preview": "vite preview",
    "spatial:run": "webspatial-builder run --base=http://localhost:5173/webspatial/avp/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --project tsconfig.app.json --noEmit"
  },
  "dependencies": {
    "@webspatial/core-sdk": "^1.1.0",
    "@webspatial/react-sdk": "^1.1.0",
    "firebase": "^10.7.1",
    "framer-motion": "^11.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^5.1.4",
    "@webspatial/builder": "^1.1.0",
    "@webspatial/platform-visionos": "^1.1.0",
    "@webspatial/vite-plugin": "^1.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "vite-plugin-html": "^3.2.2"
  }
}
